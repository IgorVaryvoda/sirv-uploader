{
  "name": "@sirv/upload-widget",
  "version": "0.1.0",
  "description": "A React file upload widget for Sirv CDN with batch uploads, CSV/Excel import, and file browser",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "optionalDependencies": {
    "exceljs": "^4.4.0",
    "heic2any": "^0.0.4"
  },
  "keywords": [
    "sirv",
    "upload",
    "file-upload",
    "react",
    "cdn",
    "image-upload",
    "batch-upload",
    "csv-import",
    "drag-drop"
  ],
  "author": "Sirv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sirv/upload-widget"
  },
  "homepage": "https://sirv.com"
}
